low: hidden;\r\n  padding: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-direction:row-reverse;\r\n}\r\n\r\n.texto-curiosidades {\r\n  max-width: 60%;\r\n  text-align: center;\r\n}\r\n\r\n.imagem-curiosidades {\r\n  float: right;\r\n  width: 30%;\r\n  height: auto;\r\n  margin-left: 20px;\r\n  margin-bottom: 10px;\r\n  border-radius: 8px;\r\n  object-fit: cover;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .container-sobre, .container-curiosidades {\r\n    flex-direction: column;\r\n    text-align: center;\r\n  }\r\n  .imagem-sobre, .imagem-curiosidades {\r\n    width: 100%;\r\n    float: none;\r\n    margin-left: 0;\r\n    margin-bottom: 20px;\r\n  }\r\n  .texto-sobre, .texto-curiosidades {\r\n    max-width: 100%;\r\n  }\r\n}\r\n\r\n@media (min-width: 769px) and (max-width: 1024px) {\r\n  .container-sobre, .container-curiosidades {\r\n    flex-direction: row;\r\n    text-align: left;\r\n  }\r\n  .imagem-sobre, .imagem-curiosidades {\r\n    width: 40%;\r\n  }\r\n  .texto-sobre, .texto-curiosidades {\r\n    max-width: 50%;\r\n  }\r\n}\r\n\r\n@media (min-width: 1025px) {\r\n  .container-sobre, .container-curiosidades {\r\n    flex-direction: row;\r\n    text-align: left;\r\n  }\r\n  .imagem-sobre, .imagem-curiosidades {\r\n    width: 30%;\r\n  }\r\n  .texto-sobre, .texto-curiosidades {\r\n    max-width: 60%;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d52d2f89f0e50ebbe41b6a78b31333e6","cacheData":{"env":{}}}